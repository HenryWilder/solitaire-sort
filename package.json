{
    "name": "solitaire-sort",
    "displayName": "Solitaire Sort",
    "description": "Sorting algorithm which plays a game of faux-solitare to order elements.",
    "author": "Henry Wilder",
    "license": "MIT",
    "version": "0.0.1",
    "repository": {
      "type": "private"
    },
	"engines": {
		"node": "*"
	},
    "main": "./out/ts/solitaire-sort",
    "scripts": {
      "vscode:prepublish": "npm run compile",
      "compile": "tsc -b",
      "watch": "tsc -b -w",
